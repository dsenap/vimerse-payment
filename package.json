{
  "name": "vimerse_frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max_old_space_size=16000\" env-cmd -f env/.env.dev next dev",
    "build:dev": "NODE_OPTIONS=\"--max_old_space_size=16000\" env-cmd -f env/.env.dev next build",
    "build": "env-cmd -f env/.env.dev next build",
    "build:prod": "env-cmd -f env/.env.prod next build && npm run create-robots-txt && npm run create-sitemap",
    "start": "next start",
    "start:dev": "env-cmd -f env/.env.dev next start",
    "start:prod": "env-cmd -f env/.env.prod next start",
    "create-robots-txt": "node node-scripts/generateRobotsTxt.js",
    "create-sitemap": "node node-scripts/generateSiteMap.js",
    "format": "prettier --ignore-path .gitignore --write \"src/**/*.+(js|jsx|scss)\"",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.{js,jsx}\" --fix --quiet",
    "extract:i18n-terms": "formatjs extract 'src/pages/terms-and-conditions.js' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/terms.json",
    "extract:i18n-privacy": "formatjs extract 'src/pages/privacy-policy.js' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/privacy.json",
    "extract:i18n-how_to_easy": "formatjs extract 'src/pages/how-to-choose-the-right-music-for-your-video.js' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/how-to-easy.json",
    "extract:i18n-how_to_choose": "formatjs extract 'src/pages/how-to-easy-write-a-youtube-video.js' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/how-to-choose.json",
    "extract:i18n-main": "formatjs extract 'src/components/**/**/*.{js,jsx,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/main.json",
    "extract:i18n-faq": "formatjs extract 'src/pages/faq.js' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/faq.json",
    "extract:i18n-about_us": "formatjs extract 'src/pages/about-us.js' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/about-us.json",
    "extract:i18n-how_to_edit_yt": "formatjs extract 'src/pages/how-to-edit-for-youtube-videos.js' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/how-to-edit-yt.json",
    "extract:i18n-how-to-sustain": "formatjs extract 'src/pages/how-to-sustain-a-youtube-channel.js' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/how-to-sustain.json",
    "extract:i18n-best-online-video": "formatjs extract 'src/pages/the-best-online-video-editor.js' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/best-online-video.json",
    "extract:i18n-make-viral-video": "formatjs extract 'src/pages/how-to-make-a-viral-video.js' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/how-to-make-viral-video.json",
    "extract:i18n": "formatjs extract 'src/{pages,components,sections}/**/**/*.{js,jsx,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/en.json",
    "compile:i18n": "formatjs compile-folder --ast --format simple content/locales content/compiled-locales",
    "i18n": "npm run extract:i18n && npm run compile:i18n",
    "storybook": "start-storybook -s ./public -p 9001",
    "build-storybook": "NODE_OPTIONS=\"--max_old_space_size=16000\" build-storybook"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@next/bundle-analyzer": "^12.1.5",
    "@u-wave/react-youtube": "^0.7.2",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "jwt-decode": "^3.1.2",
    "next": "11.0.1",
    "next-redux-wrapper": "^7.0.2",
    "next-useragent": "^2.7.0",
    "notistack": "^1.0.10",
    "query-string": "^4.3.4",
    "react": "17.0.2",
    "react-bootstrap": "^2.2.3",
    "react-calendly": "^3.0.2",
    "react-dom": "17.0.2",
    "react-ga": "^3.3.0",
    "react-ga4": "^1.4.1",
    "react-google-invisible-recaptcha": "^0.2.12",
    "react-google-login": "^5.2.2",
    "react-hook-form": "^7.29.0",
    "react-intl": "^5.20.4",
    "react-lazyload": "^3.2.0",
    "react-loader-spinner": "^5.3.4",
    "react-nextjs-toast": "^1.2.5",
    "react-redux": "^7.2.4",
    "react-scripts": "^4.0.3",
    "react-scroll": "^1.8.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@formatjs/cli": "^4.2.27",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-controls": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-knobs": "^6.3.0",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/builder-webpack5": "^6.3.4",
    "@storybook/manager-webpack5": "^6.3.4",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-formatjs": "^10.3.2",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-register": "^6.26.0",
    "cross-env": "^7.0.3",
    "css-loader": "^3.6.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-formatjs": "^2.17.6",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "next-sitemap": "^1.6.157",
    "postcss": "^8.3.6",
    "postcss-loader": "^4.3.0",
    "prettier": "^2.3.2",
    "prop-types": "^15.7.2",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.37.5",
    "sass-loader": "^12.1.0",
    "storybook-css-modules-preset": "^1.1.1",
    "style-loader": "^1.3.0",
    "webpack": "^4.46.0"
  }
}
